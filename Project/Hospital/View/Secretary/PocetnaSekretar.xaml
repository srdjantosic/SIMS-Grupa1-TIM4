<Window x:Class="Project.Hospital.View.Secretary.PocetnaSekretar"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Project.Hospital.View.Secretary"
        mc:Ignorable="d"
        Title="ZDRAVO KORPORACIJA" Height="925" Width="1024"
        WindowStartupLocation="CenterScreen">
            
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="Auto"></ColumnDefinition>
            <ColumnDefinition Width="Auto"></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <WrapPanel Grid.Column="0" Orientation="Vertical" PreviewKeyDown="WrapPanel_PreviewKeyDown" >
            <Button Height="60" Width="200" FontSize="16">Pocetna</Button>
            <Button Height="60" FontSize="16" Click="pacijenti">Pacijenti</Button>
            <Button Height="60" FontSize="16" Click="hitanSlucaj">Hitan slucaj</Button>
            <Button Height="60" FontSize="16" Click="raspored">Raspored</Button>
            <Button Height="60" FontSize="16" Click="oprema">Oprema</Button>
            <Button Height="60" FontSize="16" Click="sastanci">Sastanci</Button>
            <Button Height="60" FontSize="16" Click="zahtevi">Zahtevi za slobodne dane</Button>
            <Button Height="60" FontSize="16" Click="odjaviSe">Odjavi se</Button>
        </WrapPanel>
        <Frame x:Name="Prozor" Grid.Column="1" Width="824" Height="900"></Frame>
    </Grid>
</Window>
