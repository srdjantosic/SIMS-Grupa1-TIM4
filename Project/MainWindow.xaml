<Window x:Class="Project.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Project"
        mc:Ignorable="d"
        Title="MainWindow" Height="768" Width="1024">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Button Grid.Row="0" Height="100" Width="200" FontSize="36" Click="Button_Click_Doctor">Doctor</Button>
        <Button Grid.Row="1" Height="100" Width="200" FontSize="36" Click="Button_Click_Manager">Manager</Button>
        <Button Grid.Row="2" Height="100" Width="200" FontSize="36" Click="Button_Click">Secretary</Button>
    </Grid>
</Window>
